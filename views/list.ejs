<%- include("header") -%>

<div class="box" id="heading">
    <h1> <%= listTitle%></h1>
</div>
<div class="box">
    <% for(let i=0; i<newItemLists.length;i++){ %>
    <div class="item">
        <input type="checkbox">
        <p><%=newItemLists[i]%></p>
    </div>


    <% } %>

    <form class="item" method="post" action="/">
        <input autofocus autocomplete="off" type="text" name="newItem" placeholder="New Item">
        <button type="submit" name="list" value="<%= listTitle %>">+</button>
    </form>


</div>

<%- include("footer") -%>